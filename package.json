{
  "name": "gimmehttp",
  "author": "Brian Voelker <brian@webiswhatido.com> (http://webiswhatido.com)",
  "version": "0.0.1",
  "description": "HTTP request code generator",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brianvoe/slim-select/issues"
  },
  "keywords": [
    "http",
    "request",
    "generator",
    "multi-language"
  ],
  "repository": {
    "type": "git",
    "url": "github.com/brianvoe/gimmehttp"
  },
  "main": "index.ts",
  "scripts": {
    "dev": "vite --port=1111",
    "format": "prettier --write --cache --parser typescript \"src/**/*.ts\"",
    "lint": "prettier --write ./src/**/*",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "npm run build:clean && npm run build:docs",
    "build:clean": "rimraf ./dist/*",
    "build:docs": "vite build",
    "build:library": "npm run build:library:js",
    "build:library:js": "cd src/gimmeHTTP && rollup --config ./rollup.config.mjs && cd ../../"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/prismjs": "^1.26.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "jest": "^29.7.0",
    "prismjs": "^1.29.0",
    "sass": "^1.80.6",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vue-tsc": "^2.1.10",
    "vuex": "^4.0.2"
  }
}